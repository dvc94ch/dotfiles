# ssh private keys
.ssh/id_ed25519 filter=git-crypt diff=git-crypt
.ssh/id_rsa filter=git-crypt diff=git-crypt

# ethereum wallets
.ethereum/keystore/* filter=git-crypt diff=git-crypt

# gnucash books
.gnucash/books/*.gnucash filter=git-crypt diff=git-crypt

# muttrc
.mutt/muttrc filter=git-crypt diff=git-crypt

.authinfo filter=git-crypt diff=git-crypt
.dockercfg filter=git-crypt diff=git-crypt

# gnupg
.gnupg/*.gpg filter=git-crypt diff=git-crypt
